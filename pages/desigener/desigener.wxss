@import "../../component/template/template.wxss";
page{
  width: 100%;
  height: 100%;
  background: rgba(247, 248, 252, 1);
}
.desigener{
  width: 100%;
  height: auto;
}
.clearance{
  width: 100%;
  height: 82rpx;
}
.state{
  width: 100%;
  height: 80rpx;
  background: rgba(255, 255, 255, 1);
  position: fixed;
  left: 0;
}
.state_model{
  width: 33.33%;
  height: 100%;
  font-size:26rpx;
  line-height:36rpx;
  position: relative;
}
.state_model_coucur{
  position: absolute;
  bottom: 6rpx;
  width:88rpx;
  height:6rpx;
  background:linear-gradient(270deg,rgba(255,87,127,1) 0%,rgba(255,35,62,1) 100%);
  border-radius:3rpx;
}
.state_model_textOne{
  color:rgba(74,74,74,0.5);
}
.state_model_textTwo{
  font-weight:bold;
  color:rgba(74,74,74,1);
}
.cards{
  width: 100%;
  height: 362rpx;
  margin-top: 26rpx;
}
.cards_model{
  width: 91.47%;
  height: 100%;
  background: rgba(255, 255, 255, 1);
  border-radius: 8rpx;
}
.camo_one{
  width: 100%;
  height: 76rpx;
  border-bottom: 1rpx solid rgba(237, 238, 242, 0.5);
}
.camo_one_left{
